#!/usr/bin/env node
const program = require('commander');
const project = require("./src/project.js");
program
    .option('--name', '项目名称')
    .option('--template', '模板类型')
    .option('--url', 'git 路径')
    .parse(process.argv)
const args = program.args
const { name, template, url } = program
const projectName = args[0] || name


const project = new Project({
    projectName,
    template,
    description,
    typescript
})
project.create()
